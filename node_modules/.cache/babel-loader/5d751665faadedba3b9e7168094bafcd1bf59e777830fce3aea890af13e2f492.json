\n          v-for=\"item in navItems\"\n          :key=\"item.title\"\n          @click=\"scrollTo(item.id); drawer = false\"\n        >\n          <v-list-item-content>\n            <v-list-item-title>{{ item.title }}</v-list-item-title>\n          </v-list-item-content>\n        </v-list-item>\n      </v-list>\n    </v-navigation-drawer>\n\n    <!-- Main Content -->\n    <v-main>\n      <!-- Hero Section -->\n      <v-container fluid class=\"hero-section pa-0\">\n        <v-row no-gutters>\n          <v-col cols=\"12\">\n            <div class=\"hero-content text-center white--text py-16\">\n              <h1 class=\"display-2 font-weight-light mb-4\">\n                Model Content Protocol (MCP)\n              </h1>\n              <p class=\"headline font-weight-light\">\n                The Future of AI Integration and Content Management\n              </p>\n            </div>\n          </v-col>\n        </v-row>\n      </v-container>\n\n      <v-container class=\"py-12\">\n        <!-- What is MCP Section -->\n        <section id=\"what-is-mcp\" class=\"mb-16\">\n          <v-row justify=\"center\">\n            <v-col cols=\"12\" md=\"10\">\n              <h2 class=\"text-h3 text-center mb-8 primary--text\">\n                What is Model Content Protocol?\n              </h2>\n              <v-card elevation=\"2\" class=\"pa-8\">\n                <p class=\"text-body-1 mb-4\">\n                  The Model Content Protocol (MCP) is a standardized specification for integrating AI models with external tools and data sources. It enables seamless communication between AI systems and external services, allowing for:\n                </p>\n                <v-list>\n                  <v-list-item v-for=\"item in mcpFeatures\" :key=\"item\">\n                    <v-list-item-icon>\n                      <v-icon color=\"primary\">mdi-check-circle</v-icon>\n                    </v-list-item-icon>\n                    <v-list-item-content>\n                      <v-list-item-title>{{ item }}</v-list-item-title>\n                    </v-list-item-content>\n                  </v-list-item>\n                </v-list>\n              </v-card>\n            </v-col>\n          </v-row>\n        </section>\n\n        <!-- Architecture Section -->\n        <section id=\"architecture\" class=\"mb-16\">\n          <v-row justify=\"center\">\n            <v-col cols=\"12\" md=\"10\">\n              <h2 class=\"text-h3 text-center mb-8 primary--text\">\n                Architecture\n              </h2>\n              <v-card elevation=\"2\" class=\"pa-8\">\n                <div class=\"text-center mb-6\">\n                  <ArchitectureDiagram />\n                </div>\n                <p class=\"text-body-1 mb-4\">\n                  MCP operates on a layered architecture that includes:\n                </p>\n                <v-list>\n                  <v-list-item v-for=\"layer in architectureLayers\" :key=\"layer.name\">\n                    <v-list-item-icon>\n                      <v-icon color=\"primary\">mdi-layers</v-icon>\n                    </v-list-item-icon>\n                    <v-list-item-content>\n                      <v-list-item-title class=\"font-weight-medium\">\n                        {{ layer.name }}\n                      </v-list-item-title>\n                      <v-list-item-subtitle>\n                        {{ layer.description }}\n                      </v-list-item-subtitle>\n                    </v-list-item-content>\n                  </v-list-item>\n                </v-list>\n              </v-card>\n            </v-col>\n          </v-row>\n        </section>\n\n        <!-- Use Cases Section -->\n        <section id=\"use-cases\" class=\"mb-16\">\n          <v-row justify=\"center\">\n            <v-col cols=\"12\" md=\"10\">\n              <h2 class=\"text-h3 text-center mb-8 primary--text\">\n                Use Cases\n              </h2>\n              <v-row>\n                <v-col\n                  v-for=\"useCase in useCases\"\n                  :key=\"useCase.title\"\n                  cols=\"12\"\n                  md=\"4\"\n                >\n                  <v-card\n                    elevation=\"2\"\n                    class=\"pa-6 text-center h-100\"\n                    hover\n                  >\n                    <v-icon\n                      size=\"64\"\n                      color=\"primary\"\n                      class=\"mb-4\"\n                    >\n                      {{ useCase.icon }}\n                    </v-icon>\n                    <h3 class=\"text-h5 mb-4\">{{ useCase.title }}</h3>\n                    <p class=\"text-body-2\">{{ useCase.description }}</p>\n                  </v-card>\n                </v-col>\n              </v-row>\n            </v-col>\n          </v-row>\n        </section>\n\n        <!-- Benefits Section -->\n        <section id=\"benefits\" class=\"mb-16\">\n          <v-row justify=\"center\">\n            <v-col cols=\"12\" md=\"10\">\n              <h2 class=\"text-h3 text-center mb-8 primary--text\">\n                Benefits\n              </h2>\n              <v-card elevation=\"2\" class=\"pa-8\">\n                <v-list>\n                  <v-list-item\n                    v-for=\"benefit in benefits\"\n                    :key=\"benefit.title\"\n                    class=\"px-0\"\n                  >\n                    <v-list-item-icon>\n                      <v-icon color=\"success\">mdi-star</v-icon>\n                    </v-list-item-icon>\n                    <v-list-item-content>\n                      <v-list-item-title class=\"font-weight-bold\">\n                        {{ benefit.title }}:\n                      </v-list-item-title>\n                      <v-list-item-subtitle class=\"text-body-1 mt-1\">\n                        {{ benefit.description }}\n                      </v-list-item-subtitle>\n                    </v-list-item-content>\n                  </v-list-item>\n                </v-list>\n              </v-card>\n            </v-col>\n          </v-row>\n        </section>\n      </v-container>\n    </v-main>\n\n    <!-- Footer -->\n    <v-footer\n      color=\"primary\"\n      dark\n      padless\n    >\n      <v-col\n        class=\"text-center white--text py-4\"\n        cols=\"12\"\n      >\n        © 2025 Model Content Protocol Documentation\n      </v-col>\n    </v-footer>\n  </v-app>\n</template>\n\n<script>\nimport ArchitectureDiagram from './components/ArchitectureDiagram.vue'\n\nexport default {\n  name: 'App',\n  components: {\n    ArchitectureDiagram\n  },\n  data() {\n    return {\n      drawer: false,\n      navItems: [\n        { title: 'What is MCP?', id: 'what-is-mcp' },\n        { title: 'Architecture', id: 'architecture' },\n        { title: 'Use Cases', id: 'use-cases' },\n        { title: 'Benefits', id: 'benefits' }\n      ],\n      mcpFeatures: [\n        'Unified data access and manipulation',\n        'Standardized resource management',\n        'Consistent tool integration',\n        'Enhanced AI system capabilities'\n      ],\n      architectureLayers: [\n        {\n          name: 'Resource Layer',\n          description: 'Manages external resources and data sources'\n        },\n        {\n          name: 'Tool Layer',\n          description: 'Handles tool integration and execution'\n        },\n        {\n          name: 'Protocol Layer',\n          description: 'Standardizes communication between components'\n        }\n      ],\n      useCases: [\n        {\n          title: 'Code Development',\n          description: 'Integrate AI assistants with development tools and code repositories',\n          icon: 'mdi-code-tags'\n        },\n        {\n          title: 'Design Tools',\n          description: 'Enable AI-powered design assistance and asset management',\n          icon: 'mdi-palette'\n        },\n        {\n          title: 'Search Integration',\n          description: 'Enhance search c